
user_input = input("Enter your name:\n")
print("Your name is", user_input,sep= ' -> ', end='\n')


#take 2 inputs and add, sub,mul, div them

num1= int(input("Enter the first number:\n"))
num2= int(input("Enter the second number:\n"))
print(num1,num2)
print(type(num1))
print(type(num2))

print(num1 + num2)
print(num1 - num2)
print(num1 * num2)
print(num1 / num2)
